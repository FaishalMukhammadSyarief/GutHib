<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="CornerCut">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">10dp</item>
    </style>

    <style name="ToolbarTitle">
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">24sp</item>
        <item name="android:textColor">@color/vantablack</item>
    </style>

    <style name="Divider">
        <item name="android:layout_width">1.5dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_marginVertical">2dp</item>
        <item name="android:layout_marginHorizontal">10dp</item>
        <item name="android:background">@drawable/shape_round</item>
    </style>

</resources>